<% include ./partials/header %>
<h1>INDEX PAGE!!!</h1>

<% blogs.forEach(blog => { %>
    <h2><%= blog.title %></h2>
    <img src="<%= blog.image %>">
    <p><%- blog.body.substring(0, 100) %> ...</p>
    <p><%= blog.created.toDateString() %></p>
    <a href="/blogs/<%= blog._id %>">Read More</a>
<% }) %>
<% include ./partials/footer %>